{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:vue/base",
        "standard"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "comma-dangle": ["warn", "always-multiline"],
        "indent": ["warn", 4],
        "vue/html-indent": ["warn", 4]
    }
}
